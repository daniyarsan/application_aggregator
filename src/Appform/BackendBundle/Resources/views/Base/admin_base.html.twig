<!DOCTYPE html>
<html lang="en">
<head>
	<!-- META SECTION -->
	<title>Joli Admin - Responsive Bootstrap Admin Template</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />

	<link rel="icon" href="http://healthcaretravelers.com/favicon.ico" type="image/x-icon" />
	<!-- END META SECTION -->

	<!-- CSS INCLUDE -->
	{% block stylesheets %}
		<link rel="stylesheet" type="text/css" id="theme" href="{{ asset( 'bundles/appformbackend/css/theme-default.css' ) }}"/>
	{% endblock %}
	<!-- EOF CSS INCLUDE -->
</head>
<body>
<!-- START PAGE CONTAINER -->
<div class="page-container">
	<!-- START PAGE SIDEBAR -->
	{% include 'AppformBackendBundle:Default:sidebar.html.twig' %}
	<!-- END PAGE SIDEBAR -->
	<!-- PAGE CONTENT -->
	<div class="page-content">
		<!-- START X-NAVIGATION VERTICAL -->
		{% include 'AppformBackendBundle:Default:header.html.twig' %}
		<!-- END X-NAVIGATION VERTICAL -->
		{% block content %}{% endblock %}
	</div>
	<!-- END PAGE CONTENT -->
</div>
<!-- END PAGE CONTAINER -->

<!-- MESSAGE BOX-->
<div class="message-box animated fadeIn" data-sound="alert" id="mb-signout">
	<div class="mb-container">
		<div class="mb-middle">
			<div class="mb-title"><span class="fa fa-sign-out"></span> Log <strong>Out</strong> ?</div>
			<div class="mb-content">
				<p>Are you sure you want to log out?</p>
				<p>Press No if youwant to continue work. Press Yes to logout current user.</p>
			</div>
			<div class="mb-footer">
				<div class="pull-right">
					<a href="pages-login.html" class="btn btn-success btn-lg">Yes</a>
					<button class="btn btn-default btn-lg mb-control-close">No</button>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- END MESSAGE BOX-->

<!-- EOF CSS INCLUDE -->
<!-- START PRELOADS -->
<audio id="audio-alert" src="{{ asset( 'bundles/appformbackend/audio/alert.mp3' ) }}" preload="auto"></audio>
<audio id="audio-fail" src="{{ asset( 'bundles/appformbackend/audio/fail.mp3' ) }}" preload="auto"></audio>
<!-- END PRELOADS -->

<!-- START BASE PLUGINS -->
<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/jquery/jquery.min.js' ) }}"></script>
<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/jquery/jquery-ui.min.js' ) }}"></script>
<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/bootstrap/bootstrap.min.js' ) }}"></script>
<!-- END BASE PLUGINS -->

{% block scripts %}
	<!-- START THIS PAGE PLUGINS-->
	<script type='text/javascript' src='{{ asset( 'bundles/appformbackend/js/plugins/icheck/icheck.min.js' ) }}'></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js' ) }}"></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/scrolltotop/scrolltopcontrol.js' ) }}"></script>

	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/morris/raphael-min.js' ) }}"></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/morris/morris.min.js' ) }}"></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/rickshaw/d3.v3.js' ) }}"></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/rickshaw/rickshaw.min.js' ) }}"></script>
	<script type='text/javascript' src='{{ asset( 'bundles/appformbackend/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js' ) }}'></script>
	<script type='text/javascript' src='{{ asset( 'bundles/appformbackend/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js' ) }}'></script>
	<script type='text/javascript' src='{{ asset( 'bundles/appformbackend/js/plugins/bootstrap/bootstrap-datepicker.js' ) }}'></script>
	<script type='text/javascript' src='{{ asset( 'bundles/appformbackend/js/plugins/bootstrap/bootstrap-select.js' ) }}'></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/owl/owl.carousel.min.js' ) }}"></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/moment.min.js' ) }}"></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/daterangepicker/daterangepicker.js' ) }}"></script>
	<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins/summernote/summernote.js' ) }}"></script>
	<!-- END THIS PAGE PLUGINS-->
	<!-- END SCRIPTS -->
{% endblock %}

<!-- START TEMPLATE -->
<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/plugins.js' ) }}"></script>
<script type="text/javascript" src="{{ asset( 'bundles/appformbackend/js/actions.js' ) }}"></script>
<!-- END TEMPLATE -->
</body>
</html>