{% extends 'AppformBackendBundle:Base:admin_base.html.twig' %}

{% block content -%}
	<div class="page-title">
		<h2><span class="fa fa-arrow-circle-o-left"></span>AgencyGroup list</h2>
	</div>
	<!-- PAGE CONTENT WRAPPER -->
	<div class="page-content-wrap">
		<!-- START WIDGETS -->
		<div class="row">
			<div class="col-md-12">
				<table class="records_list">
					<thead>
					<tr>
						<th>Id</th>
						<th>Name</th>
						<th>Actions</th>
					</tr>
					</thead>
					<tbody>
					{% for entity in entities %}
						<tr>
							<td><a href="{{ path('agencygroup_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
							<td>{{ entity.name }}</td>
							<td>
								<ul>
									<li>
										<a href="{{ path('agencygroup_show', { 'id': entity.id }) }}">show</a>
									</li>
									<li>
										<a href="{{ path('agencygroup_edit', { 'id': entity.id }) }}">edit</a>
									</li>
								</ul>
							</td>
						</tr>
					{% endfor %}
					</tbody>
				</table>
				<ul>
					<li>
						<a href="{{ path('agencygroup_new') }}">
							Create a new entry
						</a>
					</li>
				</ul>
			</div>
		</div>
	</div>
{% endblock %}

{% block scripts %}
	{{ parent() }}
{% endblock %}