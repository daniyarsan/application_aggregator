{% extends 'AppformBackendBundle:Base:admin_base.html.twig' %}

{% block content -%}
	<div class="page-title">
		<h2><span class="fa fa-arrow-circle-o-left"></span>AgencyGroup</h2>
	</div>
	<!-- PAGE CONTENT WRAPPER -->
	<div class="page-content-wrap">
		<!-- START WIDGETS -->
		<div class="row">
			<div class="col-md-12">
				<table class="table rable-responsiverecord_properties">
					<tbody>
					<tr>
						<th>Id</th>
						<td>{{ entity.id }}</td>
					</tr>
					<tr>
						<th>Name</th>
						<td>{{ entity.name }}</td>
					</tr>
					<tr>
						<td>Agenices</td>
						<td>
							{% for agency in entity.agencies %}
								<a class="label label-default" href="{{ path('agency_show', {'id':agency.id}) }}">{{ agency.name }}</a>
							{% endfor %}
						</td>
					</tr>
					</tbody>
				</table>
				<a class="btn btn-default" href="{{ path('agencygroup') }}">
					Back to the list
				</a>
				<a class="btn btn-success" href="{{ path('agencygroup_edit', { 'id': entity.id }) }}">
					Edit
				</a>
			</div>
		</div>
	</div>
{% endblock %}

{% block scripts %}
	{{ parent() }}
{% endblock %}
