	{% endif %}
	{% if app.session.flashBag.has('location') %}
		{% set location = app.session.flashBag.get('location').0 %}
	{% endif %}
	<script type="text/javascript">
		$(document).ready(function () {
			setTimeout(function () {
				window.top.location.href = "http://jobsfornurses.com/jobs/";
			}, 10)
