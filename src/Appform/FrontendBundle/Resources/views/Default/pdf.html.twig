{% extends "AppformFrontendBundle::report.html.twig" %}

{% block content %}

	<!-- Header -->
	<header id="top">
		<div class="container">
			<div class="row">
				<div class="col-md-12 header">
				</div>
			</div>
		</div>

	</header>
	<section id="applicatoinForm" class="about">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h3 class="text-center">HealthCareTravelers.com (855) 3355-9924</h3>
					<h4 class="text-center">You have received another Healthcare Professional requesting more information</h4>
					<table class="table table-striped">
						<tr>
							<td>Candidate ID:</td>
							<td>{{ candidateId }}</td>
						</tr>
						<tr>
							<td>First Name:</td>
							<td>{{ firstName }}</td>
						</tr>
						<tr>
							<td>Middle Name:</td>
							<td>{{ middleName }}</td>
						</tr>
						<tr>
							<td>Last Name:</td>
							<td>{{ lastName }}</td>
						</tr>
						<tr>
							<td>Email Address:</td>
							<td>{{ email }}</td>
						</tr>
						<tr>
							<td>Phone Number:</td>
							<td>{{ phone }}</td>
						</tr>
						<tr>
							<td>Home State:</td>
							<td>{{ state }}</td>
						</tr>
						<tr>
							<td>Discipline:</td>
							<td>{{ discipline }}</td>
						</tr>
						<tr>
							<td>License State:</td>
							<td>{{ licenseState }}</td>
						</tr>
						<tr>
							<td>Specialty Primary:</td>
							<td>{{ specialtyPrimary }}</td>
						</tr>
						<tr>
							<td>Years of experience(Specialty Primary):</td>
							<td>{{ yearsLicenceSp }}</td>
						</tr>
						<tr>
							<td>Specialty Secondary:</td>
							<td>{{ specialtySecondary }}</td>
						</tr>
						<tr>
							<td>Years of experience(Specialty Secondary):</td>
							<td>{{ yearsLicenceSs }}</td>
						</tr>
						<tr>
							<td>Desired Assignement State:</td>
							<td>{{ desiredAssignementState }}</td>
						</tr>

						<tr>
							<td>Are you expereinced Traveler?</td>
							<td>{{ isExperiencedTraveler }}</td>
						</tr>
						<tr>
							<td>Are you presently on assignment?</td>
							<td>{{ isOnAssignement }}</td>
						</tr>
						<tr>
							<td>When would you like an assignment?</td>
							<td>{{ assignementTime }}</td>
						</tr>
						<tr>
							<td>Completion date:</td>
							<td>{{ completion }}</td>
						</tr>
						<tr>
							<td>Question from applicant:</td>
							<td>{{ question }}</td>
						</tr>
						<tr>
							<td>Has Resume:</td>
							<td>{{ document }}</td>
						</tr>
					</table>
				</div>

			</div>
		</div>
	</section>
{% endblock %}