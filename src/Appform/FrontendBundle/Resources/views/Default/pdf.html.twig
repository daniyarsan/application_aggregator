{% extends "AppformFrontendBundle::report.html.twig" %}

{% block content %}

	<!-- Header -->
	<header id="top">
		<div class="container">
			<div class="row">
				<div class="col-md-12 header">
				</div>
			</div>
		</div>

	</header>
	<section id="applicatoinForm" class="about">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h3>You have received another Healthcare Professional requesting more information</h3>
					<div class="row">
						<div class="col-md-4">Candidate ID</div>
						<div class="col-md-4">{{ id }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">First Name</div>
						<div class="col-md-4">{{ firstName }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Middle Name</div>
						<div class="col-md-4">{{ middleName }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Last Name</div>
						<div class="col-md-4">{{ lastName }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Email Address</div>
						<div class="col-md-4">{{ email }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Phone Number</div>
						<div class="col-md-4">{{ phone }}</div>
					</div>

					<div class="row">
						<div class="col-md-4">Home State</div>
						<div class="col-md-4">{{ state }}</div>
					</div>

					<div class="row">
						<div class="col-md-4">Specialty Primary</div>
						<div class="col-md-4">{{ specialtyPrimary }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Years of experience</div>
						<div class="col-md-4">{{ yearsLicenceSp }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Specialty Secondary</div>
						<div class="col-md-4">{{ specialtySecondary }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Years of experience</div>
						<div class="col-md-4">{{ yearsLicenceSs }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Desired Assignement State</div>
						<div class="col-md-4">{{ desiredAssignementState }}</div>
					</div>

					<div class="row">
						<div class="col-md-4">Are you expereinced Traveler?</div>
						<div class="col-md-4">{{ isExperiencedTraveler }}</div>
					</div>
					<div class="row">
						<div class="col-md-4">Are you presently on assignement?</div>
						<div class="col-md-4">{{ isOnAssignement }}</div>
					</div>

					<div class="row">
						<div class="col-md-4">Are you presently on assignement?</div>
						<div class="col-md-4">{{ assignementTime }}</div>
					</div>

					<div class="row">
						<div class="col-md-4">Are you presently on assignement?</div>
						<div class="col-md-4">{{ question }}</div>
					</div>
				</div>

			</div>
		</div>
	</section>
{% endblock %}