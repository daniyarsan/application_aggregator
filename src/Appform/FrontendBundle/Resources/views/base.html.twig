<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8"/>
	<title>{% block title %}Welcome!{% endblock %}</title>

	{% stylesheets '@AppformFrontendBundle/Resources/public/css/*' filter='cssrewrite' %}
	<link type="text/css" rel="stylesheet" href="{{ asset_url }}">
	{% endstylesheets %}


	{% javascripts '@AppformFrontendBundle/Resources/public/js/jquery.js'
					'@AppformFrontendBundle/Resources/public/js/plugins/*'
	%}
	{% block stylesheets %}{% endblock %}


	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}

	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% block content %}{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
